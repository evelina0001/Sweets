<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sweets.controllers.DeleteGiftController">
   <children>
      <GridPane layoutX="-1.0" prefHeight="404.0" prefWidth="600.0" style="-fx-background-color: #eeffc5;">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#onGoBackClick" prefHeight="43.0" prefWidth="349.0" style="-fx-background-color: white;" text="No" textFill="#e08d8d" GridPane.rowIndex="2">
               <font>
                  <Font name="Britannic Bold" size="30.0" />
               </font></Button>
            <Button mnemonicParsing="false" onAction="#onDeleteClick" prefHeight="43.0" prefWidth="339.0" style="-fx-background-color: white;" text="Yes" textAlignment="CENTER" textFill="#e08d8d" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font name="Britannic Bold" size="30.0" />
               </font></Button>
            <Label alignment="CENTER" prefHeight="183.0" prefWidth="972.0" text="Are you sure that you want to delete a gift" textFill="RED" GridPane.columnSpan="2" GridPane.rowIndex="1">
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="30.0" />
               </font></Label>
         </children>
      </GridPane>
   </children>
</AnchorPane>
